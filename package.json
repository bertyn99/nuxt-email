{
  "name": "my-module",
  "version": "1.0.0",
  "description": "My new Nuxt module",
  "repository": "your-org/my-module",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "client:dev": "nuxi dev src/devtools/client --port 3030",
    "prepare:fixtures": "nuxi prepare test/fixtures/basic",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare src/devtools/client && pnpm run prepare:fixtures",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^2.6.3",
    "@nuxt/devtools-ui-kit": "^2.6.3",
    "@nuxt/kit": "^4.0.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.2.13",
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/icon": "^2.0.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "@unocss/nuxt": "^66.5.0",
    "@unocss/preset-icons": "^66.5.0",
    "@unocss/runtime": "^66.5.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.34.0",
    "nuxt": "^4.0.3",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild"
    ]
  }
}
